http{
    upstream netmanga {
        server netmanga.herokuapp.com;
    }

    server {
        listen $PORT;
        location /static {
            alias /vol/static;
        }

        location / {
            proxy_set_header HOST $host;
            proxy_set_header X-Real-IP $remote_addr;
            proxy_pass https://netmanga;
        }
    }

    
}
