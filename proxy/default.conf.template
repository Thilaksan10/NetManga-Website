server {
    listen $PORT default_server;

    location /static {
        alias /vol/static;
    }

    location / {
        uwsgi_pass netmanga.com:$PORT;
        uwsgi_buffer_size 32k;
        uwsgi_buffers 8 32k;
        
        include /etc/nginx/uwsgi_params;
        uwsgi_read_timeout 300;
        proxy_read_timeout 300;
    }
}
